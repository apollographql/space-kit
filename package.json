{
  "name": "@apollo/space-kit",
  "version": "9.11.0",
  "description": "Home base for Apollo's design system",
  "main": "index.js",
  "scripts": {
    "prebuild": "run-p build:reset icons illustrations pictograms",
    "build": "rollup -c",
    "build:icons": "run-s icons build",
    "build:illustrations": "run-s illustrations build",
    "build:pictograms": "run-s pictograms build",
    "build:reset": "ts-node --transpile-only build-reset.ts",
    "prebundle": "run-p icons illustrations pictograms",
    "prebuild-storybook": "npm run build",
    "build-storybook": "node --max-old-space-size=6144 $(npm bin)/build-storybook --static-dir .storybook/public --quiet",
    "build:github-checks": "npx ncc build .github/actions/run-circle-ci-job/index.js -o .github/actions/run-circle-ci-job/dist",
    "clean": "run-p clean:*",
    "clean:built": "ts-node --transpile-only scripts/cleanBuiltFiles.ts",
    "clean:icons": "rimraf icons 'src/icons/!(*.story|*.spec).tsx'",
    "clean:illustrations": "rimraf illustrations 'src/illustrations/!(*.story|*.spec).tsx'",
    "clean:pictograms": "rimraf pictograms 'src/pictograms/!(*.story|*.spec).tsx'",
    "chromatic": "chromatic test",
    "netlify": "run-s build-storybook",
    "icons": "run-s clean:icons icons:generate",
    "icons:generate": "ts-node --transpile-only src/icons/scripts/convert",
    "posticons:generate": "npm run icons:create:story",
    "icons:create:story": "ts-node --transpile-only src/icons/scripts/createStory > src/icons/Icons.story.mdx",
    "illustrations": "run-s clean:illustrations illustrations:generate",
    "illustrations:generate": "ts-node --transpile-only src/illustrations/scripts/convert",
    "pictograms": "run-s clean:pictograms pictograms:generate",
    "pictograms:generate": "ts-node --transpile-only src/pictograms/scripts/convert",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx,.mdx .eslintrc.js build-reset.ts components-util/ .storybook/ stories/ src/",
    "prepare": "npm run build",
    "release": "./scripts/release.sh",
    "start": "npm run storybook",
    "prestorybook": "run-p icons:generate pictograms:generate illustrations:generate",
    "storybook": "start-storybook -p 6006 --static-dir .storybook/public",
    "test": "run-p test:*",
    "test:typescript": "tsc --noEmit",
    "test:unit": "jest",
    "test:duplicate-icons": "ts-node --transpile-only src/icons/scripts/assertNoDuplicates",
    "test:unit:coverage": "jest --collectCoverage",
    "watch": "run-p watch:*",
    "watch:rollup": "npm run build -- --watch",
    "watch:npmwatch": "npm-watch"
  },
  "watch": {
    "icons:generate": {
      "extensions": "svg,spec.tsx,.ts",
      "patterns": [
        "src/icons/svgs",
        "src/icons/scripts/convert.ts"
      ]
    },
    "illustrations:generate": {
      "extensions": "svg,spec.tsx,.ts",
      "patterns": [
        "src/illustrations/svgs",
        "src/illustrations/scripts/convert.ts"
      ]
    },
    "pictograms:generate": {
      "extensions": "svg,spec.tsx,.ts",
      "patterns": [
        "src/pictograms/svgs",
        "src/pictograms/scripts/convert.ts"
      ]
    }
  },
  "repository": "apollographql/space-kit",
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@react-aria/focus": "^3.1.0",
    "@react-aria/switch": "^3.1.0",
    "@react-aria/utils": "^3.1.0",
    "@react-aria/visually-hidden": "^3.1.0",
    "@react-stately/toggle": "^3.1.0",
    "@tippyjs/react": "^4.2.0",
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.157",
    "@types/tinycolor2": "^1.4.2",
    "classnames": "^2.2.6",
    "csstype": "^3.0.3",
    "downshift": "^6.0.6",
    "lodash": "*",
    "tinycolor2": "^1.4.1",
    "tslib": "^2.0.3",
    "use-deep-compare-effect": "^1.4.0"
  },
  "devDependencies": {
    "@actions/core": "^1.1.0",
    "@actions/github": "^1.1.0",
    "@auto-it/jira": "^9.53.1",
    "@babel/core": "^7.12.3",
    "@babel/generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/traverse": "^7.12.1",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@storybook/addon-docs": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@svgr/core": "^4.3.0",
    "@svgr/plugin-jsx": "^4.3.0",
    "@svgr/plugin-prettier": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.5.0",
    "@types/babel__traverse": "^7.0.7",
    "@types/faker": "^4.1.8",
    "@types/jest": "^26.0.15",
    "@types/less": "^3.0.0",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/rimraf": "^2.0.3",
    "@types/storybook__react": "^5.2.1",
    "@types/svgo": "^1.2.1",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "auto": "^9.53.1",
    "babel-loader": "^8.1.0",
    "btoa": "^1.2.1",
    "chromatic": "^5.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^4.1.0",
    "formik": "^2.2.5",
    "framer-motion": "1.6.8",
    "jest": "^26.5.3",
    "jest-emotion": "^10.0.32",
    "jest-util": "^26.5.2",
    "less": "^3.9.0",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "prettier": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.27.8",
    "rollup-plugin-multi-input": "^1.0.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "styled-system": "^5.0.12",
    "svgo": "^1.2.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.1-rc",
    "yup": "^0.31.0"
  },
  "peerDependencies": {
    "@emotion/cache": ">10.0.29",
    "@emotion/core": ">10.1.0",
    "framer-motion": ">=1.6",
    "react": ">=16",
    "react-dom": ">=16"
  }
}
